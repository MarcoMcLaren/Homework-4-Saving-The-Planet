<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <link rel="icon" type="image/x-icon" href="assets/favicon.ico" />
    <!-- Font Awesome icons (free version)-->
    <script src="https://use.fontawesome.com/releases/v6.1.0/js/all.js" crossorigin="anonymous"></script>
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Catamaran:100,200,300,400,500,600,700,800,900" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i" rel="stylesheet" />
    <!-- Core theme CSS (includes Bootstrap)-->
    <!-- Core theme CSS (includes Bootstrap)-->
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link rel="stylesheet" href="styles.css">
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
        <div class="container-fluid">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
          
            <div class="collapse navbar-collapse" id="navbarColor01">
                <ul class="navbar-nav me-auto">
                    <li>@Html.ActionLink("Home", "Index", "Home", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px; margin-right:30px" })</li>
                    <li>@Html.ActionLink("Insert", "Index", "InsertData", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px;  margin-right:30px" })</li>
                    <li>@Html.ActionLink("Update", "Index", "Update", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px;  margin-right:30px" })</li>
                    <li>@Html.ActionLink("Paper", "Paper", "DisplayData", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px;  margin-right:30px" })</li>
                    <li>@Html.ActionLink("Plastic", "Plastic", "DisplayData", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px;  margin-right:30px" })</li>
                    <li>@Html.ActionLink("Glass", "Glass", "DisplayData", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px;  margin-right:30px" })</li>
                    <li>@Html.ActionLink("Aluminum", "Aluminum", "DisplayData", null, new { id = "OT", style = "color: white;text-decoration: none;font-size: 25px;  margin-right:30px" })</li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
        @RenderBody()
        @*<hr />*@
        @*<footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>*@
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>



